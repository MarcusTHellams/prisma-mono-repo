{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --open --port 3009",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "lint": "eslint . --config .eslintrc.build.json --ext .ts,.tsx --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "test": "vitest --reporter=verbose",
    "test:ci": "vitest run --reporter=verbose",
    "test:coverage": "vitest --watch=false --silent --coverage --reporter=verbose"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@refinedev/chakra-ui": "^2.5.4",
    "@refinedev/cli": "^2.16.6",
    "@refinedev/core": "^4.44.5",
    "@refinedev/devtools": "^1.1.12",
    "@refinedev/inferencer": "^4.5.14",
    "@refinedev/kbar": "^1.3.4",
    "@refinedev/react-hook-form": "^4.1.2",
    "@refinedev/react-router-v6": "^4.1.0",
    "@refinedev/react-table": "^5.1.4",
    "@refinedev/simple-rest": "^4.0.0",
    "@tabler/icons": "^1.1.0",
    "axios": "^1.5.1",
    "database": "workspace:*",
    "framer-motion": "^10.13.0",
    "query-string": "7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.30.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "jsdom": "^20.0.0",
    "msw": "^1.2.3",
    "postcss": "^8.4.24",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.7.0",
    "vitest": "^0.21.1"
  }
}
